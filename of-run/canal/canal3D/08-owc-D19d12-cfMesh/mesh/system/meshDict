FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    meshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFile "canal3D.stl";

maxCellSize 0.009;

boundaryCellSize 0.0125;

minCellSize 0.00199;


localRefinement
{
    "outlet.*"
    {
        cellSize 0.0002;
	//refinementThickness 2.5;    
    } 
}
