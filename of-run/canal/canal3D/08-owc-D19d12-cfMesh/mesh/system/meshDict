renameBoundary
{
    defaultName		fixedWalls;
    defaultType		wall;

    newPatchNames
    {
        "atmosphere.*"
        {
            newName	atmosphere;
	    newType     patch;
        }
        "outflow.*"     
		{
            newName     outflow;
            newType     patch;
        } 
        "allwall.*"
        {
            newName     allwall;
            newType     wall;
