FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    meshDict;
}
// ************************************************************************* //
application     interFoam;

startFrom       startTime;

startTime           0;

stopAt              endTime;

endTime             5;

deltaT              0.01;

writeControl        adjustableRunTime;//timeStep;

writeInterval       0.5;

purgeWrite      0;
//cycleWrite          0;

writeFormat         ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable   yes;

//nCorrectors 2;
//nonOrthogonalCorrectors 0;

adjustTimeStep  on;

maxCo           1;
maxAlphaCo      1;

maxDeltaT       1;
