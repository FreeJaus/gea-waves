FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  RAS; //Reynolds-averaged simulation modelling

RAS
{
    RASModel        kEpsilon; //name of RAS turbulence model

    turbulence      on; //turn the solving of turbulence modelling

    printCoeffs     on;//print model coeffs to terminal at simulation start up
}


